{
    "extends": ["react-app", "plugin:prettier/recommended", "prettier/react"],
    "rules": {
        "react/display-name": 0,
        "prettier/prettier": 0,
        "react/no-unused-prop-types": "error",
        "import/no-anonymous-default-export": 0,
        "no-unused-vars": [
            "warn",
            { "vars": "all", "args": "after-used", "ignoreRestSiblings": true }
        ]
    },
    "env": {
        "browser": true,
        "es6": true
    }
}
